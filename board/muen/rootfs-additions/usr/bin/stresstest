#!/bin/bash

OUTPUT=/dev/ttyS0
SCRIPTDIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"

. $SCRIPTDIR/func.sh

date_print "*** Starting '$0'"

# console output
ROUNDS=100

cd /
for i in `seq 1 $ROUNDS`; do
	print "Console output round $i"
	find .
done

# stress
STRESS="stress -c 8 -m 2 --vm-bytes 64M -t 10s"
print "Executing $STRESS"
$STRESS

date_print "*** '$0' DONE"
