#!/bin/bash

OUTPUT=/dev/ttyS0
SCRIPTDIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"

. $SCRIPTDIR/func.sh

date_print "*** Starting '$0'"

# console output
ROUNDS=100

cd /
for i in `seq 1 $ROUNDS`; do
	print "Console output round $i"
	find .
done

# cpu benchmark
sysbench --test=cpu --cpu-max-prime=10000 run >$OUTPUT 2>&1

date_print "*** '$0' DONE"
